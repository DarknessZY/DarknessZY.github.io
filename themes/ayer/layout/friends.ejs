    <h1 class="page-type-title">点击进去看看吧！</h1>
    <div>
        <% if(theme.friends_link && theme.friends_link.length !=0){ %>
            <ul id="friends_link">
                <% for (var i in theme.friends_link){ %>
                    <li class="friends_li">
                        <a href="<%- url_for(theme.friends_link[i].url) %>" target="_blank" class="search-title" <% if
                            (theme.friends_link[i].description){ %>title="<%= theme.friends_link[i].description %>" <% }
